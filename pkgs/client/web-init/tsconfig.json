{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsxImportSource": "@emotion/react",
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "react": ["../../../../app/web/types/react"],
      "react/jsx-runtime": ["../../../../app/web/types/react/jsx-runtime"],
      "react-dom": ["../../../../app/web/types/react-dom"],
      "react-dom/client": ["../../../../app/web/types/react-dom/client"]
    }
  },
  "include": ["./src/**/*", "../../../../app/web/types/**/*"]
}
