{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "react": ["../../../app/web/types/react"],
      "react/jsx-runtime": ["../../../app/web/types/react/jsx-runtime"],
      "react-dom": ["../../../app/web/types/react-dom/next"]
    }
  },
  "include": ["./src/**/*", "../../../app/web/types/**/*", "../../server/web/src/auth.ts"]
}
